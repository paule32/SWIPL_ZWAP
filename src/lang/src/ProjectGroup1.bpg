#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = Editor.exe Editor.DEU Editor.ENU
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

Editor.exe: Editor.dpr
  $(DCC)

Editor.DEU: DEU\Editor.dpr
  $(DCC)

Editor.ENU: ENU\Editor.dpr
  $(DCC)


